public class MyClass {
    public String isim = "Eslem";
    public String soyisim = "Meydanlı";
    public int dogum_tarihi = 2002;
    public int bugunku_tarih = 2024;
    public int yas = bugunku_tarih-dogum_tarihi;
    public String uni = "Bandırma On Yedi Eylül Üniversitesi";
    public  String  bolum = "Bilgisayar mühendisliği";
    public String sacrengi = "Kahverengi";


}


public class MyClassDrive {
    public static void main(String[] args) {
        MyClass ornek = new MyClass();

        System.out.println(ornek.isim);
        System.out.println(ornek.soyisim);
        System.out.println(ornek.yas);
        System.out.println(ornek.uni);
        System.out.println(ornek.bolum);
        System.out.println(ornek.sacrengi);


    }
}


